{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "chrome": 60,
        "firefox": 60,
        "ie": 10
      },
      "spec": true,
      "debug": false
    }],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    ["@babel/plugin-proposal-class-properties"],
    ["@babel/plugin-transform-runtime"]
  ]
}
