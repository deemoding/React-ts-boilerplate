{
  "name": "react-ts-boilerplate",
  "version": "2.1.0",
  "main": "src/index.tsx",
  "scripts": {
    "watch": "tcm -wp src/**/*.less",
    "start": "TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack serve --color --progress",
    "cz": "git-cz",
    "lint": "tslint -e .tslintignore --fix *.ts*",
    "lint:msg": "validate-commit-msg",
    "build": "TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack --mode production --config webpack.production.config.ts",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1",
    "validate": "npm ls",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/runtime": "~7.17.9",
    "antd": "~4.19.5",
    "csstype": "~3.0.11",
    "react": "~18.0.0",
    "react-dom": "~18.0.0"
  },
  "devDependencies": {
    "@babel/core": "~7.17.9",
    "@babel/plugin-proposal-class-properties": "~7.16.7",
    "@babel/plugin-transform-runtime": "~7.17.0",
    "@babel/preset-env": "~7.16.11",
    "@babel/preset-react": "~7.16.7",
    "@babel/preset-typescript": "~7.16.7",
    "@nice-labs/ts-import-plugin": "~2.1.1",
    "@types/copy-webpack-plugin": "~8.0.1",
    "@types/html-minifier": "~4.0.2",
    "@types/minimatch": "~3.0.5",
    "@types/node": "~17.0.25",
    "@types/react": "~18.0.6",
    "@types/react-dom": "~18.0.2",
    "@types/socket.io-client": "~1.4.32",
    "@types/webpack-bundle-analyzer": "~4.4.1",
    "@types/webpack-dev-server": "~4.7.1",
    "autoprefixer": "~10.4.4",
    "babel-loader": "~8.2.5",
    "clean-webpack-plugin": "~4.0.0",
    "commitizen": "~4.2.4",
    "conventional-changelog-cli": "~2.2.2",
    "copy-webpack-plugin": "~10.2.4",
    "css-loader": "~6.7.1",
    "cz-conventional-changelog": "~3.3.0",
    "file-loader": "~6.2.0",
    "html-webpack-plugin": "~5.5.0",
    "husky": "~7.0.4",
    "less": "~4.1.2",
    "less-loader": "~10.2.0",
    "no-console-webpack-plugin": "0.0.5",
    "postcss-loader": "~6.2.1",
    "postcss-preset-env": "~7.4.3",
    "raw-loader": "~4.0.2",
    "sass-loader": "~12.6.0",
    "style-loader": "~3.3.1",
    "terser-webpack-plugin": "~5.3.1",
    "ts-loader": "~9.2.8",
    "ts-node": "~10.7.0",
    "tsconfig-paths": "~3.14.1",
    "tslint": "~5.11.0",
    "tslint-react": "~3.6.0",
    "typed-css-modules": "~0.7.1",
    "typescript": "~4.6.3",
    "url-loader": "~4.1.1",
    "validate-commit-msg": "~2.14.0",
    "webpack": "~5.72.0",
    "webpack-bundle-analyzer": "~4.5.0",
    "webpack-cli": "~4.9.2",
    "webpack-dev-server": "~4.8.1"
  },
  "keywords": [
    "es6",
    "web app",
    "typescript"
  ],
  "author": "DeemoDing",
