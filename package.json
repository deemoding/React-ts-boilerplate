{
  "name": "react-ts-boilerplate",
  "version": "2.0.0",
  "main": "src/index.tsx",
  "scripts": {
    "watch": "tcm -wp src/**/*.less",
    "start": "TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack serve --color --progress",
