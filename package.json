{
  "name": "react-ts-boilerplate",
  "version": "1.1.1",
  "main": "src/index.tsx",
  "scripts": {
    "watch": "tcm -wp src/**/*.less",
    "start": "env TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack-dev-server --progress",
    "cz": "git-cz",
    "lint": "tslint -t stylish -p tsconfig.json --fix",
    "lint:msg": "validate-commit-msg",
    "build": "env TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack --mode production --config webpack.production.config.ts",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1",
    "validate": "npm ls"
  },
  "dependencies": {
    "@babel/runtime": "^7.2.0",
    "antd": "^3.12.3",
    "csstype": "^2.6.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@nice-labs/ts-import-plugin": "^2.1.1",
    "@types/html-minifier": "^3.5.2",
    "@types/minimatch": "^3.0.3",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/socket.io-client": "^1.4.32",
    "@types/webpack": "^4.4.22",
    "@types/webpack-bundle-analyzer": "^2.13.0",
    "@types/webpack-dev-server": "^3.1.1",
    "autoprefixer": "^9.4.5",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.0",
    "commitizen": "^3.0.7",
    "conventional-changelog-cli": "~2.0.11",
    "copy-webpack-plugin": "~4.6.0",
    "css-loader": "^2.1.0",
    "cz-conventional-changelog": "^2.1.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.5.0",
    "raw-loader": "^1.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "tslint": "^5.12.1",
    "tslint-react": "^3.6.0",
    "typed-css-modules": "^0.3.7",
    "typescript": "^3.2.2",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "url-loader": "^1.1.2",
    "validate-commit-msg": "^2.14.0",
    "webpack": "^4.28.4",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-parallel-uglify-plugin": "^1.1.0"
  },
  "keywords": [
    "es6",
    "web app",
    "typescript"
  ],
  "author": "DeemoDing",
  "license": "MIT",
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert",
        "build"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
