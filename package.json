    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "~2.1.1",
    "copy-webpack-plugin": "~9.0.1",
    "css-loader": "~6.2.0",
    "cz-conventional-changelog": "^3.3.0",
    "file-loader": "^6.2.0",
